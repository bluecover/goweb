{
  "id":"YRD.InvestorProfile.NumberOfInvestorsServed.KV",
  "template": true,
  "type":"kvCard",
  "cards":[
    {
      "title": "Average",
      "content":"{{.A.data | printNumber}}",
      "type": 0
    },
    {
      "title": "Month-on-Month",
      "content":"{{.B.change}}%",
      "type": "{{.B.inc_or_dec}}"
    },
    {
      "title": "Quarter-on-Quarter",
      "content":"{{.C.change}}%",
      "type": "{{.C.inc_or_dec}}"
    }
  ],

  "#query": {
    "A": {
      "type": "element",
      "table": "YRD.investment",
      "function": "avg(repeated_investor_num)"
    },
    "B": {
      "type": "xox",
      "period": "month",
      "table": "YRD.investment",
      "function": "sum(repeated_investor_num)"
    },
    "C": {
      "type": "xox",
      "period": "quarter",
      "table": "YRD.investment",
      "function": "sum(repeated_investor_num)"
    }
  }
}